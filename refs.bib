title = {{Partial agonists and G protein-coupled receptor desensitization.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/10390646},
volume = {20},
year = {1999}
}
@article{Colgan2006,
abstract = {Nucleotides and nucleosides influence nearly every aspect of physiology and pathophysiology. Extracellular nucleotides are metabolized through regulated phosphohydrolysis by a series of ecto-nucleotidases. The formation of extracellular adenosine from adenosine 5'-monophosphate is accomplished primarily through ecto-5'-nucleotidase (CD73), a glycosyl phosphatidylinositol-linked membrane protein found on the surface of a variety of cell types. Recent in vivo studies implicating CD73 in a number of tissue protective mechanisms have provided new insight into its regulation and function and have generated considerable interest. Here, we review contributions of CD73 to cell and tissue stress responses, with a particular emphasis on physiologic responses to regulated CD73 expression and function, as well as new findings utilizing Cd73-deficient animals.},
