month = aug,
number = {5},
pages = {578--92},
pmid = {21852556},
title = {{Genetics in arterial calcification: pieces of a puzzle and cogs in a wheel.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3248761\&tool=pmcentrez\&rendertype=abstract},
